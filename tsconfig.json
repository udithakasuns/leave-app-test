// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json",     /* Recommended React Native TSConfig base */
  "compilerOptions": {
    "jsx": "react",
    /* Visit https://aka.ms/tsconfig.json to read more about this file */

    /* Completeness */
    "skipLibCheck": true,
    "noUnusedLocals": true,                                 /* Skip type checking all .d.ts files. */
    "baseUrl": ".",
    "paths" : {
      "src/*": ["src/*"],
      "assets/*": ["src/assets/*"],
      "screens/*": ["src/screens/*"],
      "components/*": ["src/components/*"],
      "navigators/*": ["src/navigators/*"],
      "services/*": ["src/services/*"],
      "utils/*": ["src/utils/*"],
      "store/*": ["src/store/*"],
      "configs/*": ["src/configs/*"],
    }
  },
}
